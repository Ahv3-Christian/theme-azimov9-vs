	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#517a8d"
			}
		},
		{
			//CONSTANTE CONST -> GALERIA <-
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
